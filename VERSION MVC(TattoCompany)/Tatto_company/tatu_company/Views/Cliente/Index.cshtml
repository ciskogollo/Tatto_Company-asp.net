@model List<tatu_company.Models.ViewModels.ListClienteViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px;">
    <a href ="~/Cliente/Nuevo"class="btn btn-primary">Nuevo</a>
      <div class="col-md-12">
         <table class=" table">
            <tr>
                <th> # </th>   
                <th>id_cliente</th>
                <th> nombre </th>
                <th> edad</th>
                <th>telefono</th>
                <th>correo</th>
            </tr>
            @foreach (var oElemento in Model)
            {
            <tr>
                <td>@oElemento.id_cliente</td>
                <td>@oElemento.nombre</td>
                <td>@oElemento.edad</td>
                <td>@oElemento.telefono</td>
                <td>@oElemento.correo</td> 

            </tr>

             }
        </table>

    </div>

</div>

